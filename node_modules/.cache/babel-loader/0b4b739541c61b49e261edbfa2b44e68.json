AkDA;AACA,QAAA,GAAA,EAAA,OAAA,CAAA,4BAAA;AADA,OAlDA;AALA,KAAA;AA8DA,GAjEA;AAmEA,EAAA,QAAA,EAAA;AACA,IAAA,WADA,yBACA;AACA,aAAA,KAAA,aAAA,IAAA,KAAA,gBAAA,GAAA,CAAA,CAAA,IAAA,KAAA,KAAA,CAAA,MAAA,GAAA,KAAA,UAAA,CAAA;AACA,KAHA;AAIA,IAAA,YAJA,0BAIA;AACA,aAAA,KAAA,aAAA,KAAA,CAAA;AACA;AANA,GAnEA;AA2EA,EAAA,OA3EA,qBA2EA,CAIA,CA/EA;AAgFA,EAAA,OAAA,EAAA;AACA,IAAA,YADA,wBACA,SADA,EACA;AACA;AACA,UAAA,SAAA,KAAA,CAAA,IAAA,CAAA,KAAA,WAAA,EAAA;AACA,aAAA,aAAA,IAAA,KAAA,gBAAA;AACA,OAFA,MAEA,IAAA,SAAA,KAAA,CAAA,CAAA,IAAA,CAAA,KAAA,YAAA,EAAA;AACA,aAAA,aAAA,IAAA,KAAA,gBAAA;AACA;AACA,KARA;AASA,IAAA,OATA,qBASA;AACA,WAAA,CAAA,GAAA,IAAA;AACA,KAXA;AAYA,IAAA,OAZA,qBAYA;AACA,WAAA,CAAA,GAAA,KAAA;AACA,KAdA;AAgBA,IAAA,GAhBA,eAgBA,KAhBA,EAgBA;AAAA;;AACA,WAAA,QAAA,CAAA,aAAA,EAAA,IAAA,EAAA;AACA,QAAA,iBAAA,EAAA,IADA;AAEA,QAAA,gBAAA,EAAA,IAFA;AAGA,QAAA,IAAA,EAAA;AAHA,OAAA,EAIA,IAJA,CAIA,YAAA;AACA,QAAA,KAAA,CAAA,KAAA,CAAA,MAAA,CAAA,KAAA,EAAA,CAAA;AACA,OANA;AAQA,KAzBA;AA0BA,IAAA,OA1BA,mBA0BA,KA1BA,EA0BA;AACA,UAAA,CAAA,GAAA,QAAA,CAAA,sBAAA,CAAA,IAAA,EAAA,CAAA,CAAA;AACA,UAAA,CAAA,GAAA,KAAA,KAAA,CAAA,KAAA,EAAA,GAAA;AACA,MAAA,CAAA,CAAA,YAAA,CAAA,KAAA,EAAA,CAAA;AAIA;AAjCA;AAhFA,CAAA","sourcesContent":["<template>\n\n  <div class=\"bottom_video\">\n    <Chat :it=\"items\"></Chat>\n    <div class=\"bottom_title\">\n      <div class=\"left_title\">\n        <div class=\"title_one\">已选择视频</div>\n        <div class=\"title_two\">(共{{items.length}}个)</div>\n      </div>\n      <div class=\"right_title\">\n        <i class=\"el-icon-warning-outline\" @mouseover=\"tipshow()\" @mouseout=\"tiphide()\"><div class=\"tips\" v-if=\"s==true\">AI标识为红色时,无法添加智能字幕;<br>译标识为红色时，无法添加译字幕;</div></i>\n        <i class=\"el-icon-plus\"></i>\n      </div>\n   \n    </div>\n    <div class=\"card-carousel-wrapper\">\n      <el-button type=\"primary\" icon=\"el-icon-arrow-left\" @click=\"moveCarousel(-1)\" :disabled=\"atHeadOfList\" class=\"arrow-left\" id=\"arrow-left\" style=\"color: #FFF;background-color: gray;border-color: gray; \"></el-button>\n      <div class=\"card-carousel\">\n        <div class=\"card-carousel--overflow-container\">\n          <div class=\"card-carousel-cards\" :style=\"{ transform: 'translateX' + '(' + currentOffset + 'px' + ')'}\">\n            <div class=\"card-carousel--card\" v-for=\"(item,index) in items\" :key=\"index\" @click=\"bigplay(index)\">\n              <i class=\"el-icon-circle-close\" @click=\"del(index)\"></i>\n              <video v-bind:src=\"item.url\" class=\"choosen\" style=\"width: 185px;height: 130px; object-fit:fill;\" ></video>\n            </div>\n          </div>\n        </div>\n      </div>\n      <el-button type=\"primary\" icon=\"el-icon-arrow-right\" @click=\"moveCarousel(1)\" :disabled=\"atEndOfList\" class=\"arrow-right\" id=\"arrow-right\" style=\"color: #FFF;background-color: gray;border-color: gray; \"></el-button>\n    </div>\n  </div>\n</template>\n\n<script>\nimport Chat from './Chat'\nexport default {\n    components:{Chat},\n    data() {\n        return {\n        currentOffset: 0,\n        windowSize: 6,\n        paginationFactor: 1290,\n        s:false,  \n        items:[\n                {\n                    url:require('../../../public/movie2.mp4'),\n                    \n                },\n                    {\n                    url:require('../../../public/movie1.mp4'),\n                    \n                },\n                    {\n                    url:require('../../../public/movie1.mp4'),\n                    \n                },\n                    {\n                    url:require('../../../public/movie2.mp4'),\n                    \n                },\n                    {\n                    url:require('../../../public/movie2.mp4'),\n                    \n                },\n                    {\n                    url:require('../../../public/movie1.mp4'),\n                    \n                },\n                {\n                    url:require('../../../public/movie1.mp4'),\n                    \n                },\n                \n                {\n                    url:require('../../../public/movie2.mp4'),\n                    \n                },\n                {\n                    url:require('../../../public/movie2.mp4'),\n                    \n                },\n                {\n                    url:require('../../../public/movie2.mp4'),\n                    \n                },\n                {\n                    url:require('../../../public/movie2.mp4'),\n                    \n                },\n                {\n                    url:require('../../../public/movie2.mp4'),\n                    \n                },\n                {\n                    url:require('../../../public/movie2.mp4'),\n                    \n                },\n                \n            ],\n        }\n    },\n    \n    computed: {\n        atEndOfList() {\n        return this.currentOffset <= (this.paginationFactor * -1) * (this.items.length - this.windowSize);\n        },\n        atHeadOfList() {\n        return this.currentOffset === 0;\n        },\n    },\n    mounted(){\n            \n            \n            \n    },\n    methods: {\n        moveCarousel(direction) {\n        // Find a more elegant way to express the :style. consider using props to make it truly generic\n        if (direction === 1 && !this.atEndOfList) {\n            this.currentOffset -= this.paginationFactor;\n        } else if (direction === -1 && !this.atHeadOfList) {\n            this.currentOffset += this.paginationFactor;\n        }\n        },\n        tipshow(){\n        this.s=true;\n        },\n        tiphide(){\n        this.s=false;\n        },\n        \n        del(index) {\n        this.$confirm('是否放弃编辑该文件, ', '提示', {\n            confirmButtonText: '确定',\n            cancelButtonText: '取消',\n            type: 'warning'\n        }).then(() => {\n            this.items.splice(index, 1)\n            });  \n        \n        },\n        bigplay(index){\n            var f=document.getElementsByClassName(\"vs\")[0]\n            var u=this.items[index].url\n            f.setAttribute(\"src\",u)\n            \n\n            \n        }\n        \n    }\n}\n</script>\n<style scoped src=\"../../bottomscoll.css\">\n\n</style>"],"sourceRoot":"src/views/edit"}]}